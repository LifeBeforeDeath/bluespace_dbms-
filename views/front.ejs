<%- include ('partials/header') %>

<div class="container">
	<header class="jumbotron">
		<div class="container">
			<h1>WELCOME TO BLUE SPACE!</h1>
			<p>AAPNI SASTI DUKAN</p>
			<p><a class="btn btn-primary btn-large" href="/front/new">add new item</a></p>
		</div>
	</header>
	<div class="row" style="display:flex; flex-wrap: wrap;">
	  <% technology.forEach(function(technology){ %> 
		<div class="col-md-3 col-sm-6">
			<div class="thumbnail">
				<img src="<%= technology.image %>">
				<div class="caption">
					<h4><%=technology.name %></h4>
				</div>
					<p>
						<a href="/front/<%=technology._id%>" class="btn btn-primary">more info</a>
					</p>
				</div>
			</div>
		<% }); %>
	</div>
</div>




<%- include ('partials/footer') %>